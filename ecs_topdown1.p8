-- ######################## game

function game_init()
  _update = game_update
  _draw = game_draw
  anim_time=0
  anim_wait=1
  world = {}
  game_over = nil
  game_win = nil
  -- init entities with ugly shortcuts to map and player for non-ecs systems
  world_map = mk_map()
  add(world, world_map)
  world_player = mk_player()
  add(world, world_player)
  -- init text sign system
  text_init()
end

function game_update()
  -- run the systems
  input(world)
  move(world)
  -- check win/lose
  if (game_over) gameover_init()
end

function game_draw()
  cls()
  -- animation
  if (time() - anim_time > anim_wait) then
    toggle_tiles()
    anim_time = time()
  end

  -- run the systems
  draw_map(world)
  sprites(world)
  show_inventory(world)
  --debug(world)

  text_draw()
end

